{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "!/license/**",
        "destination": "/html/index.html"
      },
      {
        "source": "/license/**",
        "destination": "/html/license.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 8000
    },
    "ui": {
      "enabled": true,
      "host": "localhost",
      "port": 2023
    },
    "singleProjectMode": true
  }
}
